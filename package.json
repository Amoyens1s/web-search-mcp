    "devDependencies": {
        "@types/node": "^20.14.10",
        "typescript": "^5.5.3",
        "tsx": "^4.16.2",
        "jest": "^29.7.0",
        "@types/jest": "^29.5.12",
        "eslint": "^8.57.0",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0"
    },
